#include <stdio.h>

int main() {
int a,b;
char op;
printf("enter the value of a\n" );
scanf("%d",&a);
printf("enter the value of b\n" );
scanf("%d",&b);
printf("enter the operators\n" );
scanf("\n%c",&op);
switch(op)
{
    case '+':
    printf("%d addition is", a+b);
    break ;
    case '-':
    printf("%d subtraction is", a-b);
    break ;
    case '*':
    printf("%d multiplication is", a*b);
    break ;
    case '/':
    printf("%d division is", a/b);
    break ;
    case '%':
    printf("%d modulus is", a%b);
    break ;
    
}


    return 0;
}
